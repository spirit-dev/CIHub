<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CihubCoreProject" table="cihub_core_project">
    <indexes>
      <index name="1_idx" columns="leader"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="1024" nullable="false"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="dateCreation" type="datetime" column="date_creation" nullable="true"/>
    <many-to-one field="leader" target-entity="CihubCoreUser">
      <join-columns>
        <join-column name="leader" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
