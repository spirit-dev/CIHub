<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CihubPostMaillingList" table="cihub_post_mailling_list">
    <indexes>
      <index name="todelete3_idx" columns="user_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="1024" nullable="true"/>
    <field name="surname" type="string" column="surname" length="1024" nullable="true"/>
    <field name="mail" type="string" column="mail" length="1024" nullable="false"/>
    <many-to-one field="user" target-entity="CihubCoreUser">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
